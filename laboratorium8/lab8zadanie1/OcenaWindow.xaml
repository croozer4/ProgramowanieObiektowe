        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab8zadanie1"
        mc:Ignorable="d"
        Title="OcenaWindow" Height="300" Width="300">
    <Grid>
        <DataGrid x:Name="dgOceny" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,0,0,81"/>
        <Button x:Name="btnDodajOcene" Content="Dodaj ocenę" HorizontalAlignment="Left" Margin="163,233,0,0" VerticalAlignment="Top" Width="114" Click="dodajOcene"/>

    </Grid>
