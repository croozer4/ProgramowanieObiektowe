<Window x:Class="lab8zadanie1.DodajOceneWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab8zadanie1"
        mc:Ignorable="d"
        Title="Dodaj Ocene" Height="150" Width="300">
    <Grid>
        <TextBox x:Name="tbPrzedmiot" HorizontalAlignment="Left" Margin="141,30,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="tbOcena" HorizontalAlignment="Left" Margin="141,64,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Przedmiot" HorizontalAlignment="Left" Margin="56,26,0,0" VerticalAlignment="Top" Width="80" HorizontalContentAlignment="Right"/>
        <Label Content="Ocena" HorizontalAlignment="Left" Margin="56,60,0,0" VerticalAlignment="Top" HorizontalContentAlignment="Right" Width="80"/>
        <Button x:Name="btnZatwierdzOcene" Content="Dodaj" HorizontalAlignment="Left" Margin="181,95,0,0" VerticalAlignment="Top" Width="80" Click="zatwierdzOcene"/>

    </Grid>
</Window>
